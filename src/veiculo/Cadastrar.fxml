<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="371.0" prefWidth="311.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="veiculo.Cadastrar">
    <children>
        <VBox alignment="CENTER" layoutX="76.0" layoutY="30.0" prefHeight="312.0" prefWidth="182.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Cadastrar Veiculo">
                <VBox.margin>
                    <Insets bottom="10.0" />
                </VBox.margin>
                <font>
                    <Font size="25.0" />
                </font>
            </Text>
            <children>
                <TextField fx:id="modelo" alignment="CENTER" promptText="Modelo">
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="grupo" alignment="CENTER" promptText="Grupo">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="placa" alignment="CENTER" promptText="Placa">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </TextField>
                <TextField fx:id="filial" alignment="CENTER" promptText="Filial de origem">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </TextField>
                <Button mnemonicParsing="false" onAction="#handleCadastraVeiculo" text="Cadastrar" />
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </VBox>
    </children>
</AnchorPane>
